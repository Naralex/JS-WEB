<h1>Welcome to SoftUni Wiki!</h1>
<section id="featured">
    <h2>Latest article</h2>
    <article>
        <h3>{{latest.title}}</h3>
        <p>
            {{latest.content}}
        </p>
        <div class="controls">
            <a href="/publish/display/{{latest._id}}">Read more</a>
        </div>
    </article>
</section>
<section id="recent">
    <h2>Recently added articles</h2>
    <ul>
        {{#each article}}
            <li>
                <a href="/publish/display/{{_id}}">{{title}}</a>
            </li>
        {{/each}}
    </ul>
</section>